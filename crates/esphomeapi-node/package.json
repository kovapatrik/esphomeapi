{
  "name": "@kovapatrik/esphomeapi",
  "version": "0.1.0",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "esphomeapi",
    "triples": {}
  },
  "license": "MIT",
  "ava": {
    "timeout": "3m"
  },
  "engines": {
    "node": ">= 10"
  },
  "scripts": {
    "artifacts": "pnpm exec napi artifacts",
